{
  "name": "eslint-config-cool",
  "version": "0.0.4",
  "description": "Cool's ESLint config, following our styleguide",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/RJAVA1990/eslint-config-cool"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "cool",
    "javascript",
    "styleguide",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "author": "Xuheng Liu (liuxuheng1990@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RJAVA1990/eslint-config-cool/issues"
  },
  "homepage": "https://github.com/RJAVA1990/eslint-config-cool",
  "scripts": {
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,sass,scss,less,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-angular": "^7.5.0",
    "commitizen": "^3.0.7",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.13.7"
  },
  "peerDependencies": {
    "eslint": "^5.16.0"
  },
  "engines": {
    "node": ">= 4"
  }
}
