{
  "name": "eslint-config-cool-typescript",
  "version": "0.1.0",
  "description": "Cool's ESLint config for TypeScript, following our styleguide.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/RJAVA1990/eslint-config-cool"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "cool",
    "javascript",
    "typescript",
    "styleguide",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "author": "Xuheng Liu (liuxuheng1990@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RJAVA1990/eslint-config-cool/issues"
  },
  "homepage": "https://github.com/RJAVA1990/eslint-config-cool",
  "scripts": {
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,sass,scss,less,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "eslint-config-airbnb-typescript": "^6.0.0",
    "eslint-config-cool": "^0.1.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "commitizen": "^4.0.3",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.13.7",
    "typescript": "^3.4.2"
  },
  "peerDependencies": {
    "eslint": "^5.16.0"
  },
  "engines": {
    "node": ">= 4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
